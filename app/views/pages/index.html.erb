<% content_for(:head) do %>
  <%= stylesheet_link_tag 'home.css' %>
<% end %>

<%= render "layouts/nav" %>

<% mentor_link = "https://goo.gl/forms/DmGMK7Jk5YNXfMGo1".freeze %>
<% volunteer_link = "https://goo.gl/forms/UEhrcpqhJxBqDsuF3".freeze %>

<div class="section about" id="about">
  <div class="about__wrapper">
    <div class="about__left">
      <div class="about__left-background"></div>

      <%= image_tag 'bh-logos/brickhack-6.svg', alt: 'BrickHack 6', class: 'about__logo' %>

      <p class="text subtitle"><b>February 8-9, 2020</b></p>
      <p class="text subtitle">Gordon Field House</p>
      <p class="text subtitle">Rochester Institute of Technology</p>
      <br/>

      <% if EVENT_IS_OVER %>
      <% elsif !REGISTRATION_IS_OPEN %>
        <p class="text">Registration has closed</p>
        <!-- = btn_link_to "Volunteer", volunteer_link -->
        <!-- = btn_link_to "Get Notified &raquo;".html_safe, "https://coderit.us11.list-manage.com/subscribe?u=122b09a8cef4c1f3888af8e40&id=be625a634d" -->
        <!-- = btn_link_to "Devpost", "https://brickhack4.devpost.com/" -->
        <!-- = btn_link_to "Schedule", "#schedule" -->
        <!-- %br -->
        <!-- %br -->
        <!-- %h3 Registration has closed. -->
      <% end %>

      <div>
        <%= btn_link_to "Register", "https://apply.brickhack.io" %>
        <!-- = btn_link_to "Volunteer", volunteer_link -->
      </div>
    </div>
    <div class="about__right">
      <div class="about__right-background"></div>
      <h2 class="text-center">Brick City's Premiere Hackathon</h2>
      <p class="text">
        BrickHack brings
        <b>designers, developers, and all sorts of makers</b>
        together for 24 hours to create something from nothing.
      </p>
      <p class="text">
        Mentors and industry representatives will lend expertise and watch as you dive into
        <b>learning, developing, and producing a unique project.</b>
      </p>
      <p class="text">
        Expect lots of free food, swag, coffee, and all-you-can-print 3D-printers! From the novices to the experts, from designers to engineers,
        <b>this one's for you!</b>
      </p>
    </div>
  </div>
</div>


<div class="section double-section">
  <div class="video" id="video">
    <div class="video__wrapper">
      <iframe allow="autoplay; encrypted-media" class="video__frame" frameborder="0" src="https://www.youtube-nocookie.com/embed/0fOUf1JwzBs"></iframe>
    </div>
  </div>
  <div class="what" id="what">
    <div class="what__wrapper">
      <h2 class="text-center">
        What is a <b>hackathon?</b>
      </h2>
      <p class="text">
        A Hackathon is an invention marathon.
        <b>Any student can attend!</b>
        Really, any student... and it's completely <b>free.</b>
        Don't worry about food, Internet, or travel.
      </p>
      <p class="text">
        Students work in teams and mentors are present to offer help. Sponsors attend to help, recruit, and promote their products. Teams compete in different categories and prizes are awarded, but
        <b>everyone learns and everyone wins!</b>
      </p>
      <p class="text">
        Come spend 24 hours with us at BrickHack and <b>dedicate time to learn, collaborate, build, and innovate.</b>
      </p>
      <p class="text">MLH has a great list of related questions over at <a href="https://mlh.io/faq" style="color: orange;">mlh.io/faq</a></p>
    </div>
  </div>
</div>


